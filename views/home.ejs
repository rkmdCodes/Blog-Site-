<%- include('partials/header'); -%>

<h1>Home</h1>
<p><%=startingContent%></p>
<% posts.find({},function(err,pair){ %>
    <%if(!err){%>
        <h1><%=pair.title%></h1>
        <p><%=pair.content %>
         
         <a href="/posts/<%=pair.heading%>">Read More</a>   
        </p>
    <% }else %>
         <p>err</p>
<%});%>

<%- include('partials/footer'); -%>